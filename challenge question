var heightMary = 158;
var agemary = 31;
var scoreMery=heightMary + 5 * agemary;

if(scoreJone>scoreMery && scoreJone>scoreMike){
    console.log('Jone wins the game with' + scoreJone + 'points!');
}else if(scoreMike > scoreJone && scoreMike > scoreMery){
    console,log('Mike wins the game with' + scoreMike + 'Points!');
}else if (scoreMery > scoreJone && scoreMike > scoreMike){
    console.log('Mery wins the game with' + scoreMery + 'Points!');
}else{
    console.log('It\'s a deaw.');
}
